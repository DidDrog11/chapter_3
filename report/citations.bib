@article{ilori_epidemiologic_2019,
	title = {Epidemiologic and Clinical Features of Lassa Fever Outbreak in Nigeria, January 1–May 6, 2018},
	volume = {25},
	issn = {1080-6040},
	url = {10.3201/eid2506.181035},
	doi = {10.3201/eid2506.181035},
	abstract = {The outbreak, which resulted in 423 confirmed cases and 106 deaths, was the largest recorded Lassa fever outbreak., Lassa fever ({LF}) is endemic to Nigeria, where the disease causes substantial rates of illness and death. In this article, we report an analysis of the epidemiologic and clinical aspects of the {LF} outbreak that occurred in Nigeria during January 1–May 6, 2018. A total of 1,893 cases were reported; 423 were laboratory-confirmed cases, among which 106 deaths were recorded (case-fatality rate 25.1\%). Among all confirmed cases, 37 occurred in healthcare workers. The secondary attack rate among 5,001 contacts was 0.56\%. Most (80.6\%) confirmed cases were reported from 3 states (Edo, Ondo, and Ebonyi). Fatal outcomes were significantly associated with being elderly; no administration of ribavirin; and the presence of a cough, hemorrhaging, and unconsciousness. The findings in this study should lead to further {LF} research and provide guidance to those preparing to respond to future outbreaks.},
	pages = {1066--1074},
	number = {6},
	journaltitle = {Emerging Infectious Diseases},
	shortjournal = {Emerg Infect Dis},
	author = {Ilori, Elsie A. and Furuse, Yuki and Ipadeola, Oladipupo B. and Dan-Nwafor, Chioma C. and Abubakar, Anwar and Womi-Eteng, Oboma E. and Ogbaini-Emovon, Ephraim and Okogbenin, Sylvanus and Unigwe, Uche and Ogah, Emeka and Ayodeji, Olufemi and Abejegah, Chukwuyem and Liasu, Ahmed A. and Musa, Emmanuel O. and Woldetsadik, Solomon F. and Lasuba, Clement L.P. and Alemu, Wondimagegnehu and Ihekweazu, Chikwe},
	urldate = {2020-10-21},
	date = {2019-06},
	pmid = {31107222},
	pmcid = {PMC6537738},
	file = {PubMed Central Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\SCS5KFGJ\\Ilori et al. - 2019 - Epidemiologic and Clinical Features of Lassa Fever.pdf:application/pdf},
}

@article{fichet-calvet_fluctuation_2007,
	title = {Fluctuation of abundance and Lassa virus prevalence in Mastomys natalensis in Guinea, West Africa},
	volume = {7},
	issn = {1530-3667},
	url = {https://doi.org/10.1089/vbz.2006.0520},
	doi = {10.1089/vbz.2006.0520},
	abstract = {Based on empiric surveillance data, the incidence of human Lassa fever ({LF}) cases in Guinea and other West African countries has been reported to increase during the dry season compared to the rainy season. To investigate possible links with the ecology of the rodent reservoir of the virus, we conducted a 2-year longitudinal survey of Mastomys natalensis in a region of high human Lassa virus ({LASV}) seropositivity in Guinea. Standardized rodent trapping with similar trapping efforts between seasons was performed in three villages and 53.5\% (601/1123) of the animals were identified as M. natalensis using morphometric and molecular criteria. Mean trapping success ({TS}) of M. natalensis was always higher inside houses than in proximal cultivations. In the dry season, mean {TS} increased 2-fold inside houses and decreased up to 10-fold outside (p {\textless} 0.0001), suggesting aggregation of rodents inside houses due to restricted food supply. 14.5\% (80/553) of M. natalensis were tested positive for Lassa virus by reverse transcription-polymerase chain reaction ({RT}-{PCR}; range, 5\%-30\%) and prevalence of the virus was two to three times higher in rodents captured in the rainy season than in the dry season (p {\textless} 0.05). Inside houses, however, the {LASV} prevalence fluctuated nonsignificantly with season. These data suggest that in Guinea the risk of {LASV} transmission from rodents to humans is present both in the rainy and the dry season, reflected by the occurrence of {LF} cases throughout the year. In the dry season, however, the increased risk of humans encountering Mastomys and their excreta inside of houses may result in an increase of human Lassa fever cases.},
	pages = {119--128},
	number = {2},
	journaltitle = {Vector Borne and Zoonotic Diseases},
	shortjournal = {Vector Borne Zoonotic Dis},
	author = {Fichet-Calvet, Elisabeth and Lecompte, Emilie and Koivogui, Lamine and Soropogui, Barré and Doré, Amadou and Kourouma, Fodé and Sylla, Oumar and Daffis, Stéphane and Koulémou, Kékoura and Ter Meulen, Jan},
	date = {2007},
	pmid = {17627428},
	keywords = {habitat, Ecology, Animals, Humans, Zoonoses, Rodent Diseases, Disease Reservoirs, Prevalence, Lassa virus, Lassa fever, Guinea, Lassa Fever, Rain, Seasons, Mastomys natalensis, Murinae, abundance, season, Reverse Transcriptase Polymerase Chain Reaction, Africa, prevalence, Guinea/ep [Epidemiology], Disease Reservoirs/ve [Veterinary], *Lassa Fever/ve [Veterinary], *Murinae/vi [Virology], Lassa Fever/ep [Epidemiology], Rodent Diseases/ep [Epidemiology], *Lassa Fever/tm [Transmission], *Lassa virus/ip [Isolation \& Purification], Disease Reservoirs/vi [Virology], *Zoonoses, *Rodent Diseases/tm [Transmission], human diseases, disease prevalence, disease transmission, hosts, reservoir hosts, disease vectors, vectors, trapping, disease surveys, risk, seasonality, risk assessment, Transmission, Diseases and disorders, Parasites diseases and disorders, Afrotropical region, Land zones, Population dynamics, Microbial   diseases, Habitat, Mammalia (Vertebrata).    Mastomys natalensis (Muridae)., Mammalia [Community structure /  / Small taxa] [Guinea / Faranah / ].    Mastomys natalensis [Viral diseases / Lassa virus / Seasonal prevalence, Man made   habitat},
