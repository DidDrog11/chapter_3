	keywords = {biodiversity, Bayesian hierarchical models, cross-validation, plains fish, South Platte River Basin, species distribution maps},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\TZQ6L7G4\\Broms et al. - 2016 - Model selection and assessment for multi-species o.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\45QZZ3XW\\15-1471.html:text/html},
